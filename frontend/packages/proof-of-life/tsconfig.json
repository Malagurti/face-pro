{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": false,
    "jsx": "react-jsx",
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true
  },
  "include": ["src/**/*"]
}


